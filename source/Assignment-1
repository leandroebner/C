#include <stdio.h>

int rows, collumns, i;

int main(){
  
  printf("Insert amount of row(s):\n");
  scanf("%d", &rows);
  printf("Received %d row(s).\nInsert amount of collumn(s):\n", rows);
  scanf("%d", &collumns);
  printf("Received %d collumn(s).\n\n", collumns);

  int array[collumns][rows];
  
  for(i=0; i < rows; i++){
    array[0][i] = i+1;
    printf("%d", i+1);
  }
  
  

}
